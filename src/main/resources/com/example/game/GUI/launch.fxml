<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" stylesheets="@../styling/default.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.game.launch.LaunchController">
   <children>
      <ImageView fx:id="logo" fitHeight="222.0" fitWidth="225.0" layoutX="339.0" layoutY="93.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/icon.png" />
         </image>
      </ImageView>
      <Label fx:id="title" layoutX="304.0" layoutY="330.0" text="2048 GAME">
         <font>
            <Font name="Tw Cen MT Bold" size="58.0" />
         </font>
      </Label>
      <Label fx:id="credit" layoutX="334.0" layoutY="402.0" text="Modified by Kelly Tan Kai Ling">
         <font>
            <Font name="Tw Cen MT Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="playButton" layoutX="383.0" layoutY="462.0" mnemonicParsing="false" onAction="#switchToAccount" prefHeight="46.0" prefWidth="136.0" text="PLAY" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT Bold" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
